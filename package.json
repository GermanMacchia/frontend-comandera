		"@angular/compiler-cli": "^20.1.0",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/jasmine": "~5.1.0",
		"autoprefixer": "^10.4.21",
		"cypress": "^14.5.3",
		"cypress-mochawesome-reporter": "^4.0.0",
		"jasmine-core": "~5.8.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"postcss": "^8.4.49",
		"tailwindcss": "^3.4.16",
		"typescript": "~5.8.2",
