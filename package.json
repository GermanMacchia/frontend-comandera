{
	"name": "frontend-comandera",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "cmd /c scripts\\test-full.cmd",
		"test:unitarios": "ng test --watch=false --browsers=ChromeHeadless",
