		"@angular/core": "^20.1.0",
		"@angular/forms": "^20.1.0",
		"@angular/platform-browser": "^20.1.0",
		"@angular/router": "^20.1.0",
		"@ngneat/until-destroy": "^10.0.0",
		"@ngrx/effects": "^19.2.1",
		"@ngrx/store": "^19.2.1",
		"@ngrx/store-devtools": "^19.2.1",
		"@primeng/themes": "^18.0.0",
		"ngx-permissions": "^19.0.0",
		"primeicons": "^7.0.0",
		"primeng": "^20.0.0",
		"rxjs": "~7.8.0",
		"tailwindcss-primeui": "^0.3.4",
		"tslib": "^2.3.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^20.1.4",
